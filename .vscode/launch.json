{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach to Container (QBench)",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "172.17.0.2",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "/app"
                }
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Run Interactive",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/runspace/run_interactive.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Run All",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/runspace/run_all.py",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}